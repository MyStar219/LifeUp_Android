<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/margin_x18"
    android:layout_height="@dimen/margin_x18"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/item_userfeedback_pic"
        android:layout_width="@dimen/margin_x18"
        android:layout_height="@dimen/margin_x18"
        android:layout_centerInParent="true"
        android:padding="@dimen/margin_x2"
        android:src="@drawable/user_feed_back_add_select"/>

    <ImageView
        android:id="@+id/item_userfeedback_delete_pic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/user_feed_back_delete_select"/>
</RelativeLayout>